@model ExpressVoitures.Models.Annonce

@{
    ViewData["Title"] = "Details";
}

<div>
    <h1 class="mt-4">Details de l'Annonce</h1>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.TitreAnnonce)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.TitreAnnonce)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DescriptionAnnonce)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DescriptionAnnonce)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-outline-warning" asp-action="Edit" asp-route-id="@Model?.Id">Modifier</a> |
    <a class="btn btn-outline-info" asp-action="Index" asp-route-idVoiture="@Model?.IdVoiture">Retour à l'annonce</a>
</div>
